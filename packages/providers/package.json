		"@types/debug": "^4.1.7",
		"@types/flat-cache": "^2.0.0"
	},
	"dependencies": {
		"chalk": "^4.1.2",
		"cookie": "^0.4.1",
		"debug": "^4.3.2",
		"dotenv": "^10.0.0",
		"flat-cache": "^3.0.4",
		"form-urlencoded": "^6.0.4",
		"opensubtitles-api": "^5.1.2",
		"puppeteer": "^10.2.0",
		"rimraf": "^3.0.2"
	}
}
